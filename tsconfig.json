{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/Components/Pages/BusinessPrices/priceList.jsx", "src/Components/Pages/BusinessPrices/priceList.jsx", "setupTests.js.js", "src/app/layout.js"  ],
  "exclude": [
    "node_modules"
  ],
  "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    }
}
